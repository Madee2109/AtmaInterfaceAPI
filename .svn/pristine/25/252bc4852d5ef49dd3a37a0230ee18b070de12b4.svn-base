

div.FreezeAllWork {
	display: none;
	background-image: url(transparency.png);
	background-color: rgba(0, 0, 0, 1);
	opacity: 0.8;
	width: 100%;
	height: 100vh;
	height: 100%;
	z-index: 9999;
	position: fixed;
	top: 0px;
	left: 0px;
	text-align: center;
}

div.FreezeAllWork img {
	margin-top: 15%;
	width: 15%;
}

div.custompopup {
